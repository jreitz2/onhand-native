{
  "expo": {
    "name": "On-hand Cuisine",
    "slug": "onhand-native",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/a-icon.jpg",
    "userInterfaceStyle": "light",
    "experiments": {
      "baseUrl": "/onhand-native"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jreitz2.onhandnative"
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useHermes": true,
      "package": "com.jreitz2.onhandnative"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": ["@expo/vector-icons/Ionicons"]
        }
      }
    },
    "plugins": ["expo-font"],
    "extra": {
      "eas": {
        "projectId": "e4e6591f-a31e-4add-9da0-ac587b14dfb6"
      },
      "API_KEY": "@env_API_KEY"
    }
  }
}
